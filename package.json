{
  "name": "turing-studio-backend-instance",
  "version": "1.0.0",
  "description": "Node server for Turing Studio backend instance",
  "scripts": {
    "test": "echo \"We don't need no stinkin' tests.\" && exit 0",
    "start-local": "export NODE_ENV=local && node_modules/.bin/nodemon app/server.js",
    "preinstall": "which link-module-alias && link-module-alias clean || echo",
    "postinstall": "link-module-alias"
  },
  "_moduleAliases": {
    "@config": "config",
    "@data": "data",
    "@util": "app/util",
    "@errors": "app/errors",
    "@middleware": "app/middleware",
    "@validation": "app/validation",
    "@controllers": "app/controllers",
    "@persistence": "app/persistence",
    "@tools": "tools"
  },
  "_moduleAliasIgnoreWarning": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdeanmedical/wdean-form-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wdeanmedical/wdean-form-server/issues"
  },
  "homepage": "https://github.com/wdeanmedical/wdean-form-server#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "link-module-alias": "^1.2.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "mysqldump": "^3.2.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDpendencies": {
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-security": "^1.4.0"
  }
}
